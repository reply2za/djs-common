{
  "name": "@hoursofza/djs-common",
  "version": "1.5.0",
  "description": "A common djs utility package",
  "main": "./dist/main/entry.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "prepare": "npm run build",
    "bump:patch": "npm version patch --message \"chore(release): %s\"",
    "bump:minor": "npm version minor --message \"chore(release): %s\"",
    "release": "tsc && npm publish --access public && git push",
    "release:dev": "tsc && npm publish --access public --tag dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Reply2za/djs-common.git"
  },
  "author": "Reply2za",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Reply2za/djs-common/issues"
  },
  "homepage": "https://github.com/Reply2za/djs-common#readme",
  "dependencies": {
    "discord.js": "~14.16.3"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "types": "./index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ]
}
